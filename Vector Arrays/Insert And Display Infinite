#include<iostream>
#include<vector>
using namespace std;
int main()
{
	vector <int> arr;
	int  i=0,c=0;
	do
	{
		cout<<"Enter Value to insert :";
		cin>> i;
		arr.push_back(i);
		cout<< "Enter 1 for more value ";
		cin>>c;
	}
	while(c==1);
	cout<<"enter Arrays is "<<;
	for(i=0;i<arr.size();i++)
	{
		cout<<arr.at(i)<<"  ";
	}
}

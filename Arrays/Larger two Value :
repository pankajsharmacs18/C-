#include<iostream>
using namespace std;
int main()
{

int size,i;
cout<<"enter size of arrays : ";
cin>>size;
int arr[size];
cout<<"enter value of arrys : ";
for(i=0;i<size;i++)
{
	cin>>arr[i];
}
cout<<"entetrd arrays is : ";
for(i=0;i<size;i++)
{
	cout<<arr[i]<<"  ";
}
int large=arr[0];
int secLarge=arr[0];
for(i=0;i<size;i++)
{
	if(arr[i]>large)
	{
		secLarge=large;
		large=arr[i];
	}
}
cout<<"\n Larger value is  :"<<large<<"\n Secound larger value is  : "<<secLarge;
}

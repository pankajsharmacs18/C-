#include<iostream>
#include<string>
using namespace std;
class StringImpl
{
	public: 
	
	void copyString(string str)
	{
		string str2;
		int i=0;
		while(str[i]!='\0')
		{
			str2[i]=str[i];
		i++;
		}
		cout<<"copied string is  : "<<str;
	}

};
int main()
{
StringImpl obj;
string str;
int length;
cout<<"enter string :";
getline(cin,str);
cout<<"input string is : "<<str<<endl;
obj.copyString(str);
}

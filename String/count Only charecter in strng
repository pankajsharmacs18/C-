#include<iostream>
#include<cstring>
using namespace std;
int main()
{
	
	 char str[20];
	 cout<<"Enter name of student : ";
 		cin.getline(str,20);
 	
 	cout<<"string is "<<str;
	int i=0;
	int count=0;
	while(str[i])
	{
		if((str[i]>='A' && str[i]<='Z')  || (str[i]>= 'a' && str[i] <='z'))
		{
		
		count++;
		}
	i++;
	}
	cout<<"Length is "<<count;
	
}
